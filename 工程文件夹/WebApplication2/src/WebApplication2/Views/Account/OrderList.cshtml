@model WebApplication2.Models.MemberHomeModel
@{
    ViewBag.Title = "订单一览表";
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.contBuy = "/";
}
<table>
    <td style="vertical-align:top;">
        <h3>订单一览</h3>
        <table border="1" style="border-width:1px;border-collapse:collapse;width:100%;">
            <tr>
                <th>产品名</th>
                <th>图片</th>
                <th>金额</th>
                <th>付款时间</th>
                <th>订单状态</th>
            </tr>
            @foreach (var cartItem in Model.Order)
            {
                <tr>
                    <td>@cartItem.productName</td>
                    <td><img src="/images/@cartItem.PicName" style="width:120px;height:120px" /></td>
                    <td>@cartItem.amt</td>
                    <td>@cartItem.transTime</td>
                    <td>@cartItem.orderState</td>
                </tr>
            }
        </table>
    </td>
</table>